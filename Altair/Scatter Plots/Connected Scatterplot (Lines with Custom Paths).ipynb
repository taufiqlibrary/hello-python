    "import altair as alt\n",
    "from vega_datasets import data\n",
    "\n",
    "source = data.driving()\n",
    "\n",
    "alt.Chart(source).mark_line(point=True).encode(\n",
    "    alt.X('miles', scale=alt.Scale(zero=False)),\n",
    "    alt.Y('gas', scale=alt.Scale(zero=False)),\n",
    "    order='year'\n",
    ")"
