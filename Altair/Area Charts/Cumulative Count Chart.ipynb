   "source": [
    "import altair as alt\n",
    "from vega_datasets import data\n",
    "\n",
    "source = data.movies.url\n",
    "\n",
    "alt.Chart(source).transform_window(\n",
    "    cumulative_count=\"count()\",\n",
    "    sort=[{\"field\": \"IMDB_Rating\"}],\n",
    ").mark_area().encode(\n",
    "    x=\"IMDB_Rating:Q\",\n",
    "    y=\"cumulative_count:Q\"\n",
    ")"
   ]
